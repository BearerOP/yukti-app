{
  "expo": {
    "name": "Yukti",
    "slug": "yukti",
    "version": "1.0.0",
    "main": "App.tsx",
    "orientation": "portrait",
    "icon": "./assets/images/VerticalLogo.svg",
    "scheme": "yukti",
    "userInterfaceStyle": "",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#064e3b",
      "dark": {
        "backgroundColor": "#064e3b"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bearer.yukti"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/VerticalLogo.svg",
        "backgroundColor": "#064e3b"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.svg"
    },
    "plugins": [
      "expo-web-browser"
    ]
  }
}
